// schema.prisma
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Expense {
  id        String     @default(cuid()) @id
  date      DateTime
  description   String?
  amount  Int
}

model Income {
  id        String     @default(cuid()) @id
  date      DateTime
  description   String?
  amount  Int
}